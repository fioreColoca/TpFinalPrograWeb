<tr class="text-center">
    <th scope="row">{{id}}</th>
    <td>{{cuit_cliente}}</td>
    <td>{{fecha_pedido}}</td>
    <td>{{nombre_cliente}}</td>
    <td>{{email_cliente}}</td>
    <td>{{telefono_cliente}}</td>
    <td>{{direccion_cliente}}</td>
    <td>{{contacto1}}</td>
    <td>{{contacto2}}</td>
    <td>
        <a onclick=" window.open('/pdfProforma?id={{id}}','_blank')" type="button" class="btn btn-secondary" >Ver</a>
    </td>
    <td>
        <a onclick=" window.open('/pdfReporte?id={{id}}','_blank')" type="button" class="btn btn-secondary" >Ver</a>
    </td>
    <td>
        <a data-toggle="modal" data-target="#noGrafico{{id}}" type="button" class="btn btn-outline-info"><i class="fas fa-chart-pie"></i></a>
    </td>
</tr>

<div class="modal fade" id="noGrafico{{id}}" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Podra ver los graficos una vez haya finalizado el viaje</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
        </div>
    </div>
</div>
